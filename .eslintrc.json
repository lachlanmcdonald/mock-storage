{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"@lmcd/eslint-config"
	],
	"plugins": [
		"@typescript-eslint",
		"jest"
	],
	"env": {
		"jest/globals": true
	},
	"rules": {
		"@typescript-eslint/no-explicit-any": "off"
	}
}
